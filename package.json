{
    "name": "@nexcodepl/endpoint-server",
    "version": "1.0.4",
    "description": "Package for creating endpoint on server side",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "repository": "https://github.com/NexCodePL/endpoint-server.git",
    "author": "Alexander Stefa≈Ñski <alex@nexcode.pl>",
    "license": "MIT",
    "keywords": [
        "nexcodepl",
        "endpoint",
        "server"
    ],
    "private": false,
    "scripts": {
        "build": "yarn tsc",
        "format": "yarn prettier --write \"src/**/*.ts\"",
        "lint": "yarn eslint . --ext .ts",
