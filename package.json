        "@types/node": "^14.14.3",
        "@typescript-eslint/eslint-plugin": "^4.5.0",
        "@typescript-eslint/parser": "^4.5.0",
        "eslint": "^7.12.0",
        "prettier": "^2.1.2",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.3"
    },
    "files": [
        "lib/**/*"
    ]
